    "loader-utils": "^1.1.0",
    "minimatch": "^3.0.4",
    "p-limit": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.4.0",
    "enhanced-resolve": "^3.4.1",
    "eslint": "^2.9.0",
    "is-gzip": "^2.0.0",
    "mkdirp": "^0.5.1",
    "mocha": "^2.4.5",
    "ncp": "^2.0.0",
    "rimraf": "^2.6.2",
    "standard-version": "^4.2.0"
  },
  "homepage": "https://github.com/JJJYY/import-weapp-component",
