  "license": "MIT",
  "main": "dist/index.js",
  "engines": {
    "node": ">= 4"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint src/ tests/",
    "prepare": "npm run build",
    "release": "standard-version",
